{
  "name": "vidyapati",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node -r dotenv/config ./bin/www",
    "start:dev": "NODE_ENV=development nodemon -r dotenv/config ./bin/www",
    "start:worker": "node -r dotenv/config worker/worker.js",
    "start:worker:dev": "nodemon --watch worker -r dotenv/config worker/worker.js",
    "test:integration": "NODE_ENV=test mocha test/integration --watch",
    "create:db:dev": "NODE_ENV=development knex migrate:latest && NODE_ENV=development knex seed:run"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.8.0",
    "bcryptjs": "^2.4.3",
    "bull": "^3.16.0",
    "bull-board": "^0.9.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.2",
    "lodash": "^4.17.19",
    "mkdirp": "^1.0.4",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.3.0",
    "redis": "^3.0.2",
    "replaceall": "^0.1.6",
    "simple-git": "^2.14.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "passport-stub": "^1.1.1"
  }
}
